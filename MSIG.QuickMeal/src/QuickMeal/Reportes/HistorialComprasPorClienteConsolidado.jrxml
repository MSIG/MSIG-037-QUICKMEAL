	<queryString>
		<![CDATA[select  * 
from    vw_historial_compras_x_cliente_consolidado
where   str_to_date(date_format(fecha_venta, '%d-%m-%Y %H'), '%d-%m-%Y %H') between str_to_date($P{P_FECHA_INICIAL}, '%d-%m-%Y %H') and str_to_date($P{P_FECHA_FINAL}, '%d-%m-%Y %H')
    and codigo_cliente = ifnull($P{P_CODIGO}, codigo_cliente)
    and pago = $P{P_TIPO}
    and usuario = ifnull($P{P_USUARIO}, usuario)]]>
	</queryString>
